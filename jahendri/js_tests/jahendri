		<p id="loopy">Exersice 1</p>
		
		<script>
			//document.write("hello world");
			//document.getElementById("loopy").innerHTML = "woopy";
			
			// datatype: number
			var a = 8;
			document.write("<br />datatype: number");
			document.write("<br />" + a);
			document.write("<br />" + typeof(a));
			
			//dataype: null
			b = null;
			document.write("<br />datatype: null");
			document.write("<br />" + b);
			document.write("<br />" + typeof(b));
			
			//datatype : boolean
			boolean c = fasle;
			document.write("<br />datatype: boolean");
			document.write("<br />" + c);
			document.write("<br />" + typeof(c));
			
			//datatype: float
			float d = 8.8
			document.write("<br />datatype: float");
			document.write("<br />" + d);
			document.write("<br />" + typeof(d));
			
			//datatype: char
			char e = x;
			document.write("<br />datatype: float");
			document.write("<br />" + e);
			document.write("<br />" + typeof(e));
			
			
		</script>
	</body>



